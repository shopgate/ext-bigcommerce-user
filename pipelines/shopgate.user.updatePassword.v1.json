        "input": [ {"key": "isValid", "id": "2010"} ],
        "expression": { "ok": [{"type": "input", "name": "isValid"}] },
        "then": {
          "type": "extension",
          "id": "@shopgate/bigcommerce-user",
          "path": "@shopgate/bigcommerce-user/lib/userUpdatePassword.js",
